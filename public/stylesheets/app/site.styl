// Product Image
.ImageM
  width 255px
  height 170px

.navmenu 
  font-family Helvetica, sans-serif
  color #FFFFFF !important
  font-size 13.5px
  text-transform uppercase
  font-weight bold

.menufooter 
  font-family Helvetica, sans-serif !important
  color #878787 !important
  font-size 11px


.menufooterblue 
  font-family Helvetica, sans-serif
  color #0d79f2
  font-size 11px

.popup
  display none
      
/* ICONS */  
i.icon-cart
  background-image: url("/images/buttons/blue-cart.png")
  background-position:0pt
  width:19px
  height:15px
  padding-right:5px

a:hover
  i.icon-cart
    background-image:url("/images/buttons/white-cart.png")

/* BUTTONS */
.btn-search
  background-image:  url("/images/buttons/b-search.png")
  background-repeat:  no-repeat
  background-color:  transparent
  border:  0pt none    
  width: 28px
  height: 24px

.btn-facebook
  margin:5px
  display:inline-block
  height:64px
  width:64px
  background: #3b5998
  background-image: url("/images/fb.png") /* fallback */
  background-image: -webkit-gradient(linear, left bottom, left top, color-stop(0.15, rgba(66,66,66,.6)), color-stop(0.58, rgba(255,255,255,0))), url("/images/fb.png") /* Saf4+, Chrome */
  background-image: -webkit-linear-gradient(bottom, rgba(66,66,66,.6) 15%, rgba(255,255,255,0) 58%),url("/images/fb.png") /* Chrome 10+, Saf5.1+ */
  background-image: -moz-linear-gradient(bottom, rgba(66,66,66,.6) 15%, rgba(255,255,255,0) 58%),url("/images/fb.png") /* FF3.6+ */
  background-image: -ms-linear-gradient(bottom, rgba(66,66,66,.6) 15%, rgba(255,255,255,0) 58%),url("/images/fb.png") /* IE10 */
  background-image: -o-linear-gradient(bottom, rgba(66,66,66,.6) 15%, rgba(255,255,255,0) 58%), url("/images/fb.png") /* Opera 11.10+ */
  background-image: linear-gradient(bottom, rgba(66,66,66,.6) 15%, rgba(255,255,255,0) 58%),url("/images/fb.png") /* W3C */
  -webkit-border-radius: 10px
  -moz-border-radius: 10px
  border-radius: 10px
  -webkit-box-shadow: 0px 0px 10px rgba(50, 50, 50, 0.8)
  -moz-box-shadow:    0px 0px 10px rgba(50, 50, 50, 0.8)
  box-shadow:         0px 0px 10px rgba(50, 50, 50, 0.8)
  border:0 none

  &:hover
    background-image: -webkit-gradient(linear, 0 100%, 0 0, color-stop(0.15, rgba(99,99,99,.6)), color-stop(0.58, rgba(255,255,255,.1))), url("/images/fb.png")
    background-image: -webkit-linear-gradient(bottom, rgba(99,99,99,.6) 15%, rgba(255,255,255,.1) 58%), url("/images/fb.png")
    background-image: -moz-linear-gradient(bottom, rgba(99,99,99,.6) 15%, rgba(255,255,255,.1) 58%), url("/images/fb.png")
    background-image: -ms-linear-gradient(bottom, rgba(99,99,99,.6) 15%, rgba(255,255,255,.1) 58%), url("/images/fb.png")
    background-image: -o-linear-gradient(bottom, rgba(99,99,99,.6) 15%, rgba(255,255,255,.1) 58%), url("/images/fb.png")
    background-image: linear-gradient(bottom, rgba(99,99,99,.6) 15%, rgba(255,255,255,.1) 58%), url("/images/fb.png")

